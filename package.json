{
  "name": "ct-gpt4-openai-vs-azure-openai",
  "version": "1.0.0",
  "description": "Comparing GPT-4 API results between OpenAI and Azure OpenAI Services",
  "private": true,
  "type": "module",
  "scripts": {
    "demo": "npm run prod -- ./src/index.ts",
    "dev": "cross-env NODE_ENV=development bun --bun --inspect run ",
