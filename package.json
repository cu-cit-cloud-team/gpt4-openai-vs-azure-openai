    "scratch": "npm run dev -- ./src/scratch.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "bunx --bun tsc --noEmit"
  },
  "engines": {
    "node": ">= 18.x",
    "npm": ">= 8.x",
    "bun": ">= 1.x"
  },
  "keywords": ["openai", "azure openai", "gpt-4"],
  "author": {
    "name": "Michael Sprague",
    "email": "ms388@cornell.edu"
  },
  "license": "MIT",
  "dependencies": {
    "common-tags": "1.8.2",
    "openai": "5.15.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "bun": "1.2.21",
    "bun-types": "1.2.21",
    "@biomejs/biome": "2.2.0",
    "@types/common-tags": "1.8.4",
    "@types/node": "24.3.0",
    "@types/uuid": "10.0.0",
    "cross-env": "10.0.0",
    "cz-git": "1.12.0",
    "typescript": "5.9.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  }
